{
	"name": "tauri-sveltekit-typescript-boilerplate",
	"description": "Boilerplate for Tauri and SvelteKit projects in TypeScript",
	"version": "0.0.0",
	"author": {
		"name": "Слава Соколов",
		"username": "equilpres",
		"url": "https://github.com/equilpres"
	},
	"type": "module",
	"engines": {
		"node": "^20.15"
	},
	"license": "MIT",
	"scripts": {
		"dev": "vite dev --host ::",
		"tauri:dev": "pnpm tauri dev",
		"build": "pnpm sync && pnpm lint && vite build",
		"tauri:build": "pnpm tauri build",
		"preview": "vite preview --host ::",
		"sync": "svelte-kit sync",
		"svelte-check": "svelte-check --tsconfig ./tsconfig.json",
		"lint": "svelte-check && prettier --check --cache . && eslint .",
		"fmt": "prettier --write --cache .",
		"bump": "pnpm update --latest",
		"tauri": "tauri"
	},
	"dependencies": {},
	"devDependencies": {
		"@eslint/js": "^9.5.0",
		"@sveltejs/adapter-static": "^3.0.2",
		"@sveltejs/kit": "^2.5.17",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@tauri-apps/cli": "2.0.0-beta.11",
		"@types/eslint__js": "^8.42.3",
		"autoprefixer": "^10.4.19",
		"eslint": "^9.5.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.40.0",
		"globals": "^15.6.0",
		"postcss": "^8.4.38",
		"postcss-load-config": "^6.0.1",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.5",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"svelte": "^4.2.18",
		"svelte-check": "^3.8.1",
		"tailwindcss": "^3.4.4",
		"tslib": "^2.6.3",
		"typescript": "^5.5.2",
		"typescript-eslint": "8.0.0-alpha.30",
		"vite": "^5.3.1"
	}
}
